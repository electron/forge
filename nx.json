{
  "targetDefaults": {
    "coverage:base": {
      "dependsOn": ["^coverage:base"],
      "cache": true
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": true
    },
    "test:base": {
      "dependsOn": ["^test:base"],
      "cache": true
    },
    "test:fast": {
      "dependsOn": ["^test:fast"],
      "cache": true
    },
    "test:slow": {
      "dependsOn": ["^test:slow"],
      "cache": true
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [],
    "production": ["default"]
  }
}
