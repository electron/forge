  "lint-staged": {
    "*.{html,json,md,yml}": "prettier --write",
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,lock}": "syncpack"
  },
  "prettier": {
    "singleQuote": true
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447",
  "workspaces": {
    "packages": [
      "packages/api/*",
      "packages/external/*",
      "packages/plugin/*",
      "packages/maker/*",
      "packages/publisher/*",
      "packages/template/*",
      "packages/utils/*"
    ]
  }
