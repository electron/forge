{
  "name": "electron-forge",
  "version": "6.0.0-beta.67",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">= 14.17.5"
  },
  "scripts": {
    "clean": "rimraf dist && lerna exec -- rimraf dist tsconfig.tsbuildinfo",
    "build": "lerna exec -- tsc --emitDeclarationOnly && lerna exec -- swc src --out-dir dist --quiet --extensions \".ts\" --config-file ../../../.swcrc",
    "build:full": "lerna exec -- tsc -b",
    "postbuild": "ts-node tools/test-dist",
    "coverage:fast": "xvfb-maybe cross-env INTEGRATION_TESTS=0 TS_NODE_PROJECT='./tsconfig.test.json' TS_NODE_FILES=1 nyc mocha './tools/test-globber.ts' && nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:slow": "xvfb-maybe cross-env TS_NODE_PROJECT='./tsconfig.test.json' TS_NODE_FILES=1 nyc mocha './tools/test-globber.ts' --integration && nyc report --reporter=text-lcov > coverage.lcov",
    "docs": "yarn build:full && yarn docs:generate",
    "docs:generate": "node --max-old-space-size=8192 -r ts-node/register ./tools/gen-docs.ts",
    "docs:deploy:build": "ts-node tools/sync-readmes.ts && yarn docs",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write .",
    "test": "xvfb-maybe cross-env TS_NODE_PROJECT='./tsconfig.test.json' TS_NODE_FILES=1 mocha './tools/test-globber.ts'",
    "test:fast": "xvfb-maybe cross-env TS_NODE_PROJECT='./tsconfig.test.json' TEST_FAST_ONLY=1 TS_NODE_FILES=1 mocha './tools/test-globber.ts'",
    "postinstall": "rimraf node_modules/.bin/*.ps1 && ts-node ./tools/gen-tsconfigs.ts && ts-node ./tools/gen-ts-glue.ts"
  },
  "dependencies": {
    "@electron-forge/async-ora": "file:packages/utils/async-ora",
    "@electron-forge/cli": "file:packages/api/cli",
    "@electron-forge/core": "file:packages/api/core",
    "@electron-forge/maker-appx": "file:packages/maker/appx",
    "@electron-forge/maker-base": "file:packages/maker/base",
    "@electron-forge/maker-deb": "file:packages/maker/deb",
    "@electron-forge/maker-dmg": "file:packages/maker/dmg",
    "@electron-forge/maker-flatpak": "file:packages/maker/flatpak",
    "@electron-forge/maker-pkg": "file:packages/maker/pkg",
    "@electron-forge/maker-rpm": "file:packages/maker/rpm",
    "@electron-forge/maker-snap": "file:packages/maker/snap",
    "@electron-forge/maker-squirrel": "file:packages/maker/squirrel",
    "@electron-forge/maker-wix": "file:packages/maker/wix",
    "@electron-forge/maker-zip": "file:packages/maker/zip",
    "@electron-forge/plugin-auto-unpack-natives": "file:packages/plugin/auto-unpack-natives",
    "@electron-forge/plugin-base": "file:packages/plugin/base",
    "@electron-forge/plugin-compile": "file:packages/plugin/compile",
    "@electron-forge/plugin-electronegativity": "file:packages/plugin/electronegativity",
    "@electron-forge/plugin-local-electron": "file:packages/plugin/local-electron",
    "@electron-forge/plugin-webpack": "file:packages/plugin/webpack",
    "@electron-forge/publisher-base": "file:packages/publisher/base",
    "@electron-forge/publisher-bitbucket": "file:packages/publisher/bitbucket",
    "@electron-forge/publisher-electron-release-server": "file:packages/publisher/electron-release-server",
    "@electron-forge/publisher-github": "file:packages/publisher/github",
    "@electron-forge/publisher-nucleus": "file:packages/publisher/nucleus",
    "@electron-forge/publisher-s3": "file:packages/publisher/s3",
    "@electron-forge/publisher-snapcraft": "file:packages/publisher/snapcraft",
    "@electron-forge/shared-types": "file:packages/utils/types",
    "@electron-forge/template-base": "file:packages/template/base",
    "@electron-forge/template-typescript-webpack": "file:packages/template/typescript-webpack",
    "@electron-forge/template-webpack": "file:packages/template/webpack",
    "@electron-forge/test-utils": "file:packages/utils/test-utils",
    "@electron-forge/web-multi-logger": "file:packages/utils/web-multi-logger"
  },
